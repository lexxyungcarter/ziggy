{
    "name": "lexxyungcarter/ziggy",
    "description": "Generates a Blade directive exporting all of your named Laravel routes. Also provides a nice route() helper function in JavaScript.",
    "require": {
        "laravel/framework": "~5.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Lexx YungCarter",
            "email": "lexxyungcarter@gmail.com"
        },
        {
            "name": "Daniel Coulbourne",
            "email": "daniel@tighten.co"
        },
        {
            "name": "Matt Stauffer",
            "email": "matt@tighten.co"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lexx\\Ziggy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lexx\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "~3.6",
        "mikey179/vfsStream": "^1.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lexx\\Ziggy\\ZiggyServiceProvider"
            ]
        }
    }
}
